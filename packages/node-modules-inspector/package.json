{
  "name": "node-modules-inspector",
  "type": "module",
  "version": "0.0.10",
  "description": "A Node Modules Inspector",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/node-modules-inspector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/node-modules-inspector.git"
  },
  "bugs": "https://github.com/antfu/node-modules-inspector/issues",
  "keywords": [],
  "sideEffects": false,
  "bin": "bin.mjs",
  "files": [
    "*.mjs",
    "dist"
  ],
  "scripts": {
    "dev": "pnpm run -r stub && cd src && nuxi dev",
    "build": "pnpm run wc:prepare && nuxi build src && unbuild",
    "build:debug": "NUXT_DEBUG_BUILD=true pnpm run build",
    "start": "node ./bin.mjs",
    "prepack": "pnpm build",
    "prepare": "nuxi prepare src && pnpm run wc:prepare",
    "wc:prepare": "rollup -c",
    "wc:build": "pnpm run wc:prepare && NMI_BACKEND=webcontainer nuxi build src && unbuild",
    "wc:dev": "pnpm run -r stub && pnpm run wc:prepare && cd src && NMI_BACKEND=webcontainer nuxi dev"
  },
  "dependencies": {
    "birpc": "catalog:",
    "cac": "catalog:",
    "debug": "catalog:",
    "fast-glob": "catalog:",
    "flatted": "catalog:",
    "get-port-please": "catalog:",
    "h3": "catalog:",
    "launch-editor": "catalog:",
    "mlly": "catalog:",
    "mrmime": "catalog:",
    "node-modules-tools": "workspace:*",
    "open": "catalog:",
    "pathe": "catalog:",
    "picocolors": "catalog:",
    "structured-clone-es": "catalog:",
    "ws": "catalog:"
  },
  "devDependencies": {
    "@types/picomatch": "catalog:",
    "@types/semver": "^7.5.8",
    "@unocss/nuxt": "catalog:",
    "@vueuse/nuxt": "catalog:",
    "@webcontainer/api": "catalog:",
    "@xterm/addon-fit": "catalog:",
    "@xterm/xterm": "catalog:",
    "d3": "catalog:",
    "d3-hierarchy": "catalog:",
    "d3-shape": "catalog:",
    "floating-vue": "catalog:",
    "fuse.js": "catalog:",
    "modern-screenshot": "catalog:",
    "picomatch": "catalog:",
    "semver": "^7.7.1",
    "shiki": "catalog:",
    "theme-vitesse": "catalog:",
    "vite-hot-client": "catalog:"
  }
}
